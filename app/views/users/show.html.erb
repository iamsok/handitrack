<h3><%= @user.name%></h3>
<h6><%= "#{@user.city.titleize}, #{@user.state.capitalize} #{@user.zip_code}" %></h6>

<h4> Current Handicap <%= @user.handicap %>

<table>
  <tr>
    <th>Golf Course</th>
    <th>Score</th>
  </tr>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.tee_rating.golf_course.name %> </td>
        <td><%= score.score %></td>
      </tr>
    <% end %>
</table>

<% if user_signed_in? && current_user == @user %>
  <%= link_to 'Edit Profile', edit_user_registration_path %>
<% end %>
