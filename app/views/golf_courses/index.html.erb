 <h1>Golf Courses</h1>


<p><%= link_to "Add a new score", new_score_path %></p>

<ol>
 <h3> List of golf courses </h3>
 <% @golf_courses.each do |golf_course| %>
  <li><%= link_to golf_course.name, golf_course_path(golf_course) %> - <%= golf_course.course_rating %> - <%= golf_course.slope_rating %></li>
 <% end %>
</ol>
